{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./documentation"
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "lumen",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
